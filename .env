# n8n database config
DB_TYPE=postgresdb
DB_POSTGRESDB_HOST=${PGHOST}
DB_POSTGRESDB_PORT=${PGPORT}
DB_POSTGRESDB_DATABASE=${POSTGRES_DB}
DB_POSTGRESDB_USER=${POSTGRES_USER}
DB_POSTGRESDB_PASSWORD=${POSTGRES_PASSWORD}

# Optional: disable SQLite fallback
DB_SQLITE_DATABASE=

# n8n instance base URL (adjust for your domain if custom)
N8N_HOST=0.0.0.0
N8N_PORT=5678
WEBHOOK_TUNNEL_URL=https://${RAILWAY_STATIC_URL}

# Optional: enable basic auth login setup
N8N_BASIC_AUTH_ACTIVE=true
N8N_BASIC_AUTH_USER=admin
N8N_BASIC_AUTH_PASSWORD= Hassanpolito12$

# Optional: disable telemetry
N8N_DIAGNOSTICS_ENABLED=false
